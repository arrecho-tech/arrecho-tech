# For running with docker-compose (payload container connects to service hostname `postgres`)
DATABASE_URL=postgres://postgres:postgres@postgres:5432/arrecho-tech

# Payload secret (set to any long random string)
PAYLOAD_SECRET=YOUR_SECRET_HERE

# Vercel Blob (set by Vercel automatically when you enable Blob storage)
BLOB_READ_WRITE_TOKEN=

# SMTP (Proton SMTP submission tokens)
SMTP_HOST=smtp.protonmail.ch
SMTP_PORT=587
SMTP_USER=
SMTP_PASS=
SMTP_FROM_NAME=Arrecho Tech
SMTP_FROM_ADDRESS=
